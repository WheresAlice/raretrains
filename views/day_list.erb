<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3><%= filter %> <%= list_type %>s at <%= station %> on <%= day %></h3>
        <span class="pull-right clickable"><i class="glyphicon glyphicon-chevron-up"></i></span>
      </div>
      <div class="panel-body">
        <table class="table table-striped table-condensed sortable-theme-bootstrap" data-sortable id="<%= list_type %>s">
          <thead>
          <tr>
            <th class='sort-default'><%= list_type %></th>
            <th>Count</th>
          </tr>
          </thead>
          <tbody>
          <% items.each do |item| %>
              <tr>
                <td><a href="/<%= station_code %>/services?<%= uri_part %>=<%= URI.encode(item[0].to_s, /\W/) %>&date=<%= day %>" class="serviceslink"><%= item[0] %></a></td>
                <td><%= item[1] %></td>
              </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>